<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-feature android:name="android.hardware.camera" />

    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.TemanSawit"
        tools:targetApi="31">
        <activity
            android:name=".ui.screen.camera.views.HasilActivity"
            android:exported="false"
            android:theme="@style/Theme.AppCompat"/>
        <activity
            android:name=".ui.screen.camera.views.CameraTActivity"
            android:exported="false"
            android:theme="@style/Theme.AppCompat" />
        <activity
            android:name=".ui.screen.camera.views.DeteksiActivity"
            android:exported="false"
            android:theme="@style/Theme.AppCompat" />
        <activity
            android:name=".ui.screen.camera.views.CameraXActivity"
            android:exported="true"
            android:theme="@style/Theme.AppCompat" />
        <activity
            android:name=".ui.screen.profile.AboutUsActivty"
            android:exported="false"
            android:label="AboutUsActivity"
            android:theme="@style/Theme.TemanSawit" />
        <activity
            android:name=".ui.screen.profile.ChangePasswordActivity"
            android:exported="false"
            android:label="ChangePasswordActivity"
            android:theme="@style/Theme.TemanSawit" />
        <activity
            android:name=".ui.screen.profile.ContactUsActivity"
            android:exported="false"
            android:label="ContactUsActivity"
            android:theme="@style/Theme.TemanSawit" />
        <activity
            android:name=".ui.screen.profile.EditProfileActivity"
            android:exported="false"
            android:label="EditProfileActivity"
            android:theme="@style/Theme.TemanSawit" />
        <activity
            android:name=".ui.screen.profile.PrivasiPolicyActivity"
            android:exported="false"
            android:label="PrivasiPolicyActivity"
            android:theme="@style/Theme.TemanSawit" />
        <activity
            android:name=".ui.screen.profile.TermsandConditionsActivity"
            android:exported="false"
            android:label="TermsandConditionsActivity"
            android:theme="@style/Theme.TemanSawit" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.App.Starting">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>